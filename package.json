  "main": "index.js",
  "scripts": {
    "start-dev": "concurrently 'npm run runclient' 'npm run runserver'",
    "runserver": "cross-env NODE_ENV=development nodemon --exec babel-node -r dotenv/config server/server.js dotenv_config_path=./server/config/.env.development",
    "runclient": "cross-env NODE_ENV=development webpack serve --config ./webpack.config.dev.js --mode development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/index.js",
    "previous-build": "webpack --mode production",
    "previous-dev": "webpack --mode development && node server/index.js",
    "build": "webpack --mode production",
    "clean": "rm -rf dist",
    "dev": "cross-env webpack serve --config ./webpack.config.dev.js --mode development && NODE_ENV=development babel-node server/server.js",
    "prod": "cross-env webpack --config ./webpack.config.prod.js --mode production && NODE_ENV=production babel-node server/server.js"
  },
